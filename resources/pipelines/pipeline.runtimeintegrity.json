{
  "pipeline":[
    {
     "processor": "sysflowreader",
     "handler": "flattener",
     "in": "sysflow sysflowchan",
     "out": ["flat flattenerchan"]
    },
    {
     "processor": "policyengine",
     "in": "flat flattenerchan",
     "out": ["evt eventchan"],
     "policies": "../resources/policies/runtimeintegrity"     
    },
    {
     "processor": "exporter",
     "in": "evt eventchan",
     "export": "syslog",     
     "proto": "tcp",
     "tag": "sysflow",
     "host": "localhost",
     "port": "514"    
    }
  ]
}
